{
  "name": "@achamaro/tailwindcss-iconify-icon",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.5.4",
  "description": "Tailwind CSS plugin for Iconify Icon",
  "keywords": [
    "tailwindcss",
    "iconify",
    "icon"
  ],
  "type": "module",
  "author": "achamaro",
  "license": "MIT",
  "homepage": "https://github.com/achamaro/tailwindcss-plugins/tree/main/packages/iconify-icon#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/achamaro/tailwindcss-plugins.git",
    "directory": "packages/iconify-icon"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./tailwind-merge-plugin": {
      "types": "./dist/tailwind-merge-plugin.d.ts",
      "import": "./dist/tailwind-merge-plugin.js",
      "require": "./dist/tailwind-merge-plugin.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "tailwind-merge-plugin": [
        "dist/tailwind-merge-plugin.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run"
  },
  "dependencies": {
    "fast-glob": "^3.3.2",
    "svgo": "^3.3.2",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "iconify-icon": "^2.2.0",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "tsconfig": "workspace:*",
    "tsup": "^8.3.5",
    "utility": "workspace:^",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "tailwind-merge": "*",
    "tailwindcss": "*"
  },
  "peerDependenciesMeta": {
    "tailwind-merge": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    }
  },
  "packageManager": "pnpm@8.6.0",
  "volta": {
    "node": "18.16.0"
  }
}
